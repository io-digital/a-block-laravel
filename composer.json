{
    "name": "io-digital/a-block-laravel",
    "description": "A Laravel wrapper for io-digital/a-block-php",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "blockchain",
        "ablock",
        "laravel"
    ],
    "autoload": {
        "psr-4": {
            "IODigital\\ABlockLaravel\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Rian Zietsman",
            "email": "rian@io.co.za",
            "role": "developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "io-digital/a-block-php": "^0.1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "IODigital\\ABlockLaravel\\Providers\\ABlockServiceProvider"
            ],
            "aliases": {
                "AWallet": "IODigital\\ABlockLaravel\\Facades\\AWalletFacade"
            }
        }
    }
}